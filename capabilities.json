{
    "dataRoles": [
        {
            "displayName": "Category Data",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Tooltips",
            "name": "Tooltips",
            "kind": "Measure"
        }
    ],
    "objects": {
        "colorSelector": {
            "properties": {
                "fillColor": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "linePointSelector": {
            "properties": {}
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 30000
                        }
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "measure"
                            },
                            "dataReductionAlgorithm": {
                                "window": {
                                    "count": 30000
                                }
                            }
                        }
                    ]
                }
            }
        }
    ],
    
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "Tooltips"
        ],
        "supportEnhancedTooltips": true
        
    },
    "privileges": []
}